
 <ul class="collection with-header">

  <li class="collection-header"><h4>Vehículos</h4></li>
    <% @vehicles.each do |vehicle| %>
      <li class="collection-item avatar">
        <img src="https://images.vexels.com/media/users/3/127711/isolated/preview/384e0b3361d99d9c370b4037115324b9-flat-vintage-car-icon-by-vexels.png" alt="" class="circle">
        <span class="title"><%= link_to vehicle.model, vehicle, :"data-turbolinks" => false %></span>
          <p><%= vehicle.brand %><br>
            <%= vehicle.year %><br>
            <%= link_to 'Editar', edit_vehicle_path(vehicle) %>
          </p>
          <%= link_to '<i class="secondary-content material-icons">delete</i>'.html_safe, vehicle, method: :delete, data: { confirm: '¿Estás seguro?' } %>
      </li>
    <%end%>
 </ul>

<br>

<%= link_to 'Nuevo vehículo', new_vehicle_path %>
