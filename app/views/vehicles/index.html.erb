<p id="notice"><%= notice %></p>

<h1>Vehículos</h1>

<table>
  <thead>
    <tr>
      <!--<th>User</th>-->
      <th>Marca</th>
      <th>Modelo</th>
      <th>Año</th>
      <th>Kilometros por litro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <!--<td><%= vehicle.user %></td>-->
        <td><%= vehicle.brand %></td>
        <td><%= vehicle.model %></td>
        <td><%= vehicle.year %></td>
        <td><%= vehicle.fuel_efficiency %></td>
        <td><%= link_to 'Ver', vehicle %></td>
        <td><%= link_to 'Editar', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Eliminar', vehicle, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo vehículo', new_vehicle_path %>
